@using TravelTripProje.Models.Entities
@model List<Blog>

<div class="box1-left">
    @foreach (var item in Model)
    {
        <div class="bot-img">
            <img src="@item.BlogImage" alt="Alternate Text" />
        </div>
        <div class="bot-desc1">
            <p>@item.Aciklama.Substring(0,50)</p>
        </div>
        <div class="clear"></div>
    }
</div>